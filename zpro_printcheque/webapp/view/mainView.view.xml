<mvc:View xmlns:form="sap.ui.layout.form" controllerName="zmc.exim.pro.zproprintcheque.controller.mainView"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core"
	xmlns:f="sap.ui.layout.form"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content >
            <VBox class="sapUiSmallMargin">
		<f:SimpleForm id="SimpleFormChange354wideDual"
			editable="true"
			layout="ResponsiveGridLayout"
			title="Print Cheque"
			labelSpanXL="4"
			labelSpanL="3"
			labelSpanM="4"
			labelSpanS="12"
			adjustLabelSpan="false"
			emptySpanXL="0"
			emptySpanL="4"
			emptySpanM="0"
			emptySpanS="0"
			columnsXL="2"
			columnsL="1"
			columnsM="1"
			singleContainerFullSize="false" >
			<f:content>
				<Label text="{i18n>docno}" />
				<Input placeholder="Enter Document Number From" >
                <layoutData>
						<l:GridData span="XL3 L2 M3 S5" />
					</layoutData>
				</Input>
				<Input placeholder="Enter Document Number To" >
					<layoutData>
						<l:GridData span="XL3 L2 M3 S5" />
					</layoutData>
				</Input>

				<Label text="{i18n>date}" />
				<DatePicker >
					<layoutData>
						<l:GridData span="XL3 L2 M3 S5" />
					</layoutData>
				</DatePicker>
				<DatePicker >
					<layoutData>
						<l:GridData span="XL3 L2 M3 S5" />
					</layoutData>
				</DatePicker>
				

				<Label text="{i18n>compcode}" />
				<Input placeholder="Enter Company Code" >
					<layoutData>
						<l:GridData span="XL3 L2 M3 S5" />
					</layoutData>
				</Input>
				<Input placeholder="Enter Company Code" >
					<layoutData>
						<l:GridData span="XL3 L2 M3 S5" />
					</layoutData>
				</Input>
				<Label text="{i18n>fiscyear}" />
				<DatePicker valueFormat="yyyy" displayFormat="yyyy" >
					<layoutData>
						<l:GridData span="XL3 L2 M3 S5" />
					</layoutData>
				</DatePicker>
				<DatePicker valueFormat="yyyy" displayFormat="yyyy"  >
					<layoutData>
						<l:GridData span="XL3 L2 M3 S5" />
					</layoutData>
				</DatePicker>
				<Label text="{i18n>doctype}" />
				<Input placeholder="Enter Document Type" >
					<layoutData>
						<l:GridData span="XL3 L2 M3 S5" />
					</layoutData>
				</Input>
				<Input placeholder="Enter Document Type" >
					<layoutData>
						<l:GridData span="XL3 L2 M3 S5" />
					</layoutData>
				</Input>
                <Button text="Go" type="Emphasized"></Button>
			</f:content>
		</f:SimpleForm>
	</VBox>
        <Table mode="SingleSelectLeft">
            <t
            <columns>
                <Column >
                    <Title text="{i18n>docno}"></Title>
                </Column>
                <Column >
                    <Title text="{i18n>compcode}"></Title>
                </Column>
                <Column >
                    <Title text="{i18n>year}"></Title>
                </Column>
                <Column >
                    <Title text="{i18n>amnt}"></Title>
                </Column>
            </columns>
            <ColumnListItem >
                <cells>
                    <Text text="1"></Text>
                </cells>
            </ColumnListItem>
            <ColumnListItem >
                <cells>
                    <Text text="0000"></Text>
                </cells>
            </ColumnListItem>
            <ColumnListItem >
                <cells>
                    <Text text="2024"></Text>
                </cells>
            </ColumnListItem>
            <ColumnListItem >
                <cells>
                    <Text text="1000.00"></Text>
                </cells>
            </ColumnListItem>
        </Table>
        </content>
    </Page>
</mvc:View>
