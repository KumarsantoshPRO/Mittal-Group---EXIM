<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <ViewSettingsDialog confirm="handleFilterDialogConfirm">
        <filterItems>
            <ViewSettingsFilterItem
                text="Document No"
                key="DocumentNo"
            >
                <items>
                    <ViewSettingsItem
                        text="1500000017"
                        key="1500000017"
                    />
                    <ViewSettingsItem
                        text="1500000018"
                        key="1500000018"
                    />
                </items>
            </ViewSettingsFilterItem>
            <ViewSettingsFilterItem
                text="Company"
                key="Company"
                multiSelect="false"
            >
                <items>
                    <ViewSettingsItem
                        text="1000"
                        key="1000"
                    />
                    <ViewSettingsItem
                        text="1001"
                        key="1001"
                    />
                </items>
            </ViewSettingsFilterItem>
			 <ViewSettingsFilterItem
                text="Year"
                key="Year"
                
            >
                <items>
                    <ViewSettingsItem
                        text="2024"
                        key="2024"
                    />
