<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        id="idSDPortOfLoadingF4"
        title="Port of Loading"
        noDataText="No Data"
        search="onValueHelpSearch_PortOfLoading"
        liveChange="onValueHelpSearch_PortOfLoading"
        confirm="onValueHelpConfirm_PortOfLoading"
        contentWidth="250px" 
    >
        <items>
            <StandardListItem
                id="idSLPortOfLoadingValueHelp"
                title="{BillingDocument}"
                description="{BillingDocumentItem}"
                wrapping="true"
                
            />
        </items>
    </SelectDialog>
</core:FragmentDefinition>
