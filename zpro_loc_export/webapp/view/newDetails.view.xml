<mvc:View
    controllerName="zmc.exim.pro.sk.zprolocexport.controller.Details"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:form="sap.ui.layout.form"
    xmlns:uxap="sap.uxap"
>
    <uxap:ObjectPageLayout
        id="ObjectPageLayout"
        upperCaseAnchorBar="true"
        showFooter="true"
    >
    <uxap:headerTitle  >
            <uxap:ObjectPageHeader objectTitle= "">
               
                <!-- <Button text="Edit" type="Emphasized" press="onEdit" visible="{=${GlobalModel>/Editable}===false? true:false}"></Button>
            <Button
                    text="Back"
                    type="Back"
                    press="onBack"
                /> -->
            </uxap:ObjectPageHeader>
        </uxap:headerTitle>
        <uxap:sections>
            <uxap:ObjectPageSection
                id="idOPSHeaderDetails"
                titleUppercase="true"
                title="General Details"
            >
                <uxap:subSections>
                    <uxap:ObjectPageSubSection
                        id="idV2OPSubHeaderDetails"
                        title="General Details"
                        titleUppercase="true"
                    >
                        <uxap:blocks>
                            <form:SimpleForm
                                editable="true"
                                class="sapUiResponsiveContentPadding"
                              layout="ColumnLayout"
                                columnsL="2"
                                columnsM="2"
                                columnsXL="2"
                            >
                                <form:content>
                                    <core:Title text="LC Number: -------" />
                                    <Label class="boldLabel" text='Sales Contract' />
                                    <Input width="80%" />
                                    <!-- <Text text="ABC" visible="{oViewModel>/TextVisible}"/> -->
                                    <Label text='Sales order' class="boldLabel" />
                                    <Input width="80%" />
                                    <Label
                                        text='Shipmet Last Date  - Original' class="boldLabel"
                                    />
                                    <Input width="80%" />
                                    <Label
                                        text='Latest document date - Original' class="boldLabel"
                                    />
                                    <Input width="80%" />
                                    <Label
                                        text='Shipmet Last Date  - Amendment' class="boldLabel"
                                    />
                                    <Input width="80%" />
                                    <Label
                                        text='Latest document date - Amendment' class="boldLabel"
                                    />
                                    <Input width="80%" />
                                    <Label text='LC Issue Date - Amendment' class="boldLabel"/>
                                    <Input width="80%" />
                                    <Label
                                        text='LC issuance Bank Account number' class="boldLabel"
                                    />
                                    <Input width="80%" />
                                    <Label text='LC issuance Bank Name' class="boldLabel"/>
                                    <Input width="80%" />
                                    <Label text='lC issuance Swift code' class="boldLabel"/>
                                    <Input width="80%" />
                                    <Label
                                        text='Beneficiary Bank details with swift code' class="boldLabel"
                                    />
                                    <Input width="80%" />
                                    <Label
                                        text='Negotiating Bank details with swift code' class="boldLabel"
                                    />
                                    <Input width="80%" />
                                    <!-- 12 -->
                                    <Label text='Port of Loading ' class="boldLabel"/>
                                    <Input width="80%" />
                                    <Label text='Final Destination ' class="boldLabel"/>
                                    <Input width="80%" />
                                    <Label text='Issue Charges by Seller' class="boldLabel"/>
                                    <Input width="80%" />
                                    <core:Title />
                                    <Label
                                        text='Confirmation charges by Seller' class="boldLabel"
                                    />
                                    <Input width="80%" />
                                    <Label
                                        text='Discounting charges by Seller' class="boldLabel"
                                    />
                                    <Input width="80%" />
                                    <Label text='Issue Charges by buyer' class="boldLabel"/>
                                    <Input width="80%" />
                                    <Label
                                        text='Confirmation charges by buyer' class="boldLabel"
                                    />
                                    <Input width="80%" />
                                    <Label
                                        text='Discounting charges by buyer' class="boldLabel"
                                    />
                                    <Input width="80%" />
                                    <Label text='Payment Terms ' class="boldLabel"/>
                                    <Input width="80%" />
                                    <Label text='Tolerance Qty % ' class="boldLabel"/>
                                    <Input width="80%" />

                                </form:content>
                            </form:SimpleForm>
                        </uxap:blocks>
                    </uxap:ObjectPageSubSection>
                </uxap:subSections>
            </uxap:ObjectPageSection>
            <uxap:ObjectPageSection
                id="idOPSHeaderDetails2"
                titleUppercase="true"
                title="Other Details"
            >
                <uxap:subSections>
                    <uxap:ObjectPageSubSection
                        id="idV2OPSubHeaderDetails2"
                        title="Other Details"
                        titleUppercase="true"
                    >
                        <uxap:blocks>
                            <form:SimpleForm
                                editable="true"
                                class="sapUiResponsiveContentPadding"
                                layout="ColumnLayout"
                                columnsL="2"
                                columnsM="2"
                                columnsXL="2"
                            >
                                <form:content>
                                    <core:Title text="LC Number: -------" />
                                    
                                    <Label text='Shipment Type'  class="boldLabel"/>
                                    <Input width="80%" />

                                    <Label text='LC type ' class="boldLabel"/>
                                    <Input width="80%" />
                                    <!-- 24 -->
                                    <Label text='PI' />
                                    <Input width="80%" />
                                    <Label text='LC Expiry Date - Original' class="boldLabel"/>
                                    <Input width="80%" />
                                    <Label text='LC Issue Date - Original' class="boldLabel"/>
                                    <Input width="80%" />
                                    <Label
                                        text='LC receving  Date  - Original' class="boldLabel"
                                    />
                                    <Input width="80%" />
                                    <Label text='LC Expiry Date - Amendment' class="boldLabel"/>
                                    <Input width="80%" />
                                    <Label
                                        text='LC receving  Date  - Amendment' class="boldLabel"
                                    />
                                    <Input width="80%" />
                                    <Label
                                        text='LC Recieiving Bank Account number ' class="boldLabel"
                                    />
                                    <Input width="80%" />
                                    <core:Title />
                                    <Label text='LC Receiving Bank Name' class="boldLabel"/>
                                    <Input width="80%" />
                                    <Label text='lC Receving Swift code' class="boldLabel"/>
                                    <Input width="80%" />
                                    <Label
                                        text='Confirmation Bank details with swift code' class="boldLabel"
                                    />
                                    <Input width="80%" />
                                    <Label text='Port of Discharge ' class="boldLabel"/>
                                    <Input width="80%" />

                                    <Label text='LC value ' class="boldLabel"/>
                                    <Input width="80%" />
                                    <!-- 36 -->
                                    <Label text='List of documents under LC' class="boldLabel"/>
                                    <Input width="80%" />
                                    <Label
                                        text='Amendment  charges by Seller' class="boldLabel"
                                    />
                                    <Input width="80%" />
                                    <Label
                                        text='Discrepency charges by Seller' class="boldLabel"
                                    />
                                    <Input width="80%" />
                                    <Label text='Total Charges by Seller' class="boldLabel"/>
                                    <Input width="80%" />
                                    <Label
                                        text='Amendment  charges by Seller' class="boldLabel"
                                    />
                                    <Input width="80%" />
                                    <Label
                                        text='Discrepency charges by buyer' class="boldLabel"
                                    />
                                    <Input width="80%" />
                                    <Label text='Total Charges by buyer' class="boldLabel"/>
                                    <Input width="80%" />
                                    <Label text='LC Charges  in India' class="boldLabel"/>
                                    <Input width="80%" />
                                    <Label text='Tolerance Value % ' class="boldLabel"/>
                                    <Input width="80%" />
                                    <Label text='Trans-Shipment ' class="boldLabel" />
                                    <Input width="80%" />
                                </form:content>
                            </form:SimpleForm>
                        </uxap:blocks>
                    </uxap:ObjectPageSubSection>
                </uxap:subSections>
            </uxap:ObjectPageSection>

            <uxap:ObjectPageSection
                id="idV2OPSItemsDetail"
                titleUppercase="true"
                title="Items Details"
            >
                <uxap:subSections>
                    <uxap:ObjectPageSubSection
                        id="idV2OPSubItemsDetail"
                        title="Items Details"
                        titleUppercase="true"
                    >
                        <uxap:blocks>
                            <IconTabBar
                                id="idIconTabBarNoIcons"
                                expanded="{device>/isNoPhone}"
                                class="sapUiNoContentPadding"
                                headerBackgroundDesign="Solid"
                            >
                                <items>
                                    <IconTabFilter
                                        text="Original"
                                        iconColor="Contrast"
                                    >
                                        <core:Fragment
                                            id="idFrg_OriginalDetials"
                                            fragmentName="zmc.exim.pro.sk.zprolocexport.view.fragments.details.originalDetails"
                                            type="XML"
                                        />
                                    </IconTabFilter>
                                    <IconTabFilter
                                        text="Amendment"
                                        iconColor="Contrast"
                                    >
                                        <core:Fragment
                                            id="idFrg_AmendmentDetials"
                                            fragmentName="zmc.exim.pro.sk.zprolocexport.view.fragments.details.amendmentDetails"
                                            type="XML"
                                        />
                                    </IconTabFilter>
                                </items>
                            </IconTabBar>
                        </uxap:blocks>
                    </uxap:ObjectPageSubSection>
                </uxap:subSections>
            </uxap:ObjectPageSection>
        </uxap:sections>

        <uxap:footer>
            <Bar id="idV2Bar">
                <contentRight>
                    <Button
                        id="idBtn_Cancel"
                        text="Cancel"
                        type="Reject"
                    />
                    <Button
                        id="idBtn_SaveLCDetails"
                        text="Save"
                        type="Accept"
                    />
                </contentRight>
            </Bar>
        </uxap:footer>
    </uxap:ObjectPageLayout>
</mvc:View>
