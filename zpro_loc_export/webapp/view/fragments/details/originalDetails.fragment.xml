<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    xmlns:u="sap.ui.unified"
>
    <Table
        id="idTbl_"
        mode="Delete"
        sticky="ColumnHeaders,HeaderToolbar"
        growing="true"
    >
        <headerToolbar>
            <OverflowToolbar>
               
                <Button
                    tooltip="Mutiselect"
                    icon="sap-icon://multiselect-none"
                    press="handleMultiSelectButtonPressed"
                    visible="false"
                />
                <Button
                    tooltip="Sort"
                    icon="sap-icon://sort"
                    press="handleSortButtonPressed"
                    visible="false"
                />
                <Button
                    tooltip="Filter"
                    icon="sap-icon://filter"
                    press="handleFilterButtonPressed"
                    visible="false"
                />
                <Button
                    tooltip="Group"
                    icon="sap-icon://group-2"
                    press="handleGroupButtonPressed"
                    visible="false"
                />
                <ToolbarSpacer />
                <Button
                    id="idBtnAddNew"
                    text="Add new"
                    press="onAddNew"
                    type="Emphasized"
                    tooltip="Add new row"
                    icon="sap-icon://add"
                />
            </OverflowToolbar>
        </headerToolbar>
        <columns>
            <Column>
                <Text text="SO Items" />
            </Column>

            <Column >
                <Text text="Export Material " />
            </Column>
            <Column >
                <Text text="Material Description " />
            </Column>
            <Column>
                <Text text="LC Qty" />
            </Column>
            <Column>
                <Text text="Per unit price" />
            </Column>
            <Column>
                <Text text="UOM" />
            </Column>
            <Column>
                <Text text="Total  Value " />
            </Column>
            <Column>
                <Text text="SO currency " />
            </Column>
            <Column>
                <Text text="Exchange Rate " />
            </Column>
            <Column>
                <Text text="Total INR value " />
            </Column>
        </columns>

        <ColumnListItem>
            <cells>
                <Input visible="{oViewModel>/EditVisible}" />
                <Input visible="{oViewModel>/EditVisible}" />
                <Input visible="{oViewModel>/EditVisible}" />
                <Input visible="{oViewModel>/EditVisible}" />
                <Input visible="{oViewModel>/EditVisible}" />
                <Input visible="{oViewModel>/EditVisible}" />
                <Input visible="{oViewModel>/EditVisible}" />
                <Input visible="{oViewModel>/EditVisible}" />
                <Input visible="{oViewModel>/EditVisible}" />
                <Input visible="{oViewModel>/EditVisible}" />
                <Text visible="{oViewModel>/TextVisible}" />
                <Text visible="{oViewModel>/TextVisible}" />
                <Text visible="{oViewModel>/TextVisible}" />
                <Text visible="{oViewModel>/TextVisible}" />
                <Text visible="{oViewModel>/TextVisible}" />
                <Text visible="{oViewModel>/TextVisible}" />
                <Text visible="{oViewModel>/TextVisible}" />
                <Text visible="{oViewModel>/TextVisible}" />
                <Text visible="{oViewModel>/TextVisible}" />
                <Text visible="{oViewModel>/TextVisible}" />
                <Text visible="{oViewModel>/TextVisible}" />
                
                          </cells>
        </ColumnListItem>
    </Table>
</core:FragmentDefinition>
